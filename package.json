{
  "name": "wan-web-wallet",
  "version": "1.0.1",
  "main": "./build/index.js",
  "files": [
    "build",
    "index.css"
  ],
  "scripts": {
    "build": "rm -rf build && tsc",
    "example": "npm --prefix ./examples/%example% install && npm --prefix ./examples/%example% start",
    "example:basic": "cross-env NODE_ENV=development example=\"basic\" npm run example:start",
    "example:start": "cross-env NODE_ENV=development npm --prefix ./examples/basic start",
    "prepublish": "npm run build",
    "estart": "npm run build && yarn && npm run rm && npm run copybuild && npm run example:start",
    "rm": "rm -rf ./examples/basic/node_modules/wan-web-wallet/build ./examples/basic/node_modules/wan-web-wallet/index.css",
    "copybuild": "cp -r ./{build,index.css} ./examples/basic/node_modules/wan-web-wallet"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@ledgerhq/hw-app-eth": "^4.46.0",
    "@ledgerhq/hw-transport-u2f": "^4.46.0",
    "@types/js-base64": "^2.3.1",
    "await-lock": "^1.1.3",
    "bignumber.js": "^8.1.1",
    "clipboard-copy": "^2.0.0",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "6.1.0",
    "ethers-wan": "^4.0.40",
    "immutable": "^3.8.2",
    "js-base64": "^2.5.1",
    "qr-image": "^3.2.0",
    "react-paginate": "^6.3.0",
    "trezor-connect": "^8.0.6",
    "walletlink": "^1.0.0",
    "wanchaints-tx": "^0.0.2",
    "web3": "^1.2.8"
  },
  "devDependencies": {
    "@types/ethereumjs-tx": "^1.0.1",
    "@types/node": "11.13.2",
    "@types/qr-image": "^3.2.1",
    "@types/react": "16.8.6",
    "@types/react-paginate": "^6.2.1",
    "@types/react-redux": "^7.0.2",
    "@types/request": "^2.48.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^6.0.3",
    "ethereumjs-util": "6.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "request": "^2.88.0",
    "typescript": "3.4.2"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "redux": "^3.0.0 || ^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wandevs/wandex-sdk-wallets.git"
  },
